{
    "name": "octolize\/wp-octolize-docs-chat",
    "description": "Octolize docs chat",
    "license": "none",
    "keywords": [
        "wordpress",
        "woocommerce",
        "chat"
    ],
    "homepage": "https:\/\/gitlab.wpdesk.dev\/wpdesk\/predators\/library\/wp-octolize-docs-chat",
    "authors": [
        {
            "name": "grola",
            "email": "grola@octolize.com"
        }
    ],
    "config": {
        "gitlab-domains": [
            "gitlab.wpdesk.dev"
        ],
        "platform": {
            "php": "7.4"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "wpdesk\/wp-show-decision": "^1.1"
    },
    "require-dev": {
        "phpunit\/phpunit": "^7||^8||^9",
        "mockery\/mockery": "*",
        "10up\/wp_mock": "*",
        "phpcompatibility\/php-compatibility": "^9.1",
        "php-stubs\/woocommerce-stubs": "^8.3",
        "php-stubs\/wordpress-stubs": "^6.3",
        "wpdesk\/wp-plugin-flow": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "FSVendor\\Octolize\\Docs\\Chat\\": "src\/Chat"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/unit"
        ]
    },
    "extra": {
        "text-domain": "wp-octolize-docs-chat",
        "translations-folder": "lang",
        "po-files": {
            "pl_PL": "pl_PL.po"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit-unit": "phpunit --configuration phpunit-unit.xml --coverage-text --colors=never",
        "phpunit-unit-fast": "phpunit --configuration phpunit-unit.xml --no-coverage",
        "phpunit-integration": "phpunit --configuration phpunit-integration.xml --coverage-text --colors=never",
        "phpunit-integration-fast": "phpunit --configuration phpunit-integration.xml --no-coverage"
    },
    "repositories": {
        "octolize": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        },
        "wpdesk": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        }
    }
}