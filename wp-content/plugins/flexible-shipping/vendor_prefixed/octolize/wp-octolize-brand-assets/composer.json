{
    "name": "octolize\/wp-octolize-brand-assets",
    "description": "Octolize Brand Assets",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "woocommerce",
        "shipping"
    ],
    "homepage": "https:\/\/gitlab.wpdesk.dev\/wpdesk\/predators\/library\/wp-octolize-brand-assets",
    "authors": [
        {
            "name": "grola",
            "email": "grola@octolize.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*",
        "php": ">=7.4",
        "wpdesk\/wp-plugin-flow-common": "^1.1",
        "wpdesk\/wp-show-decision": "^1.1"
    },
    "require-dev": {
        "phpunit\/phpunit": "<7",
        "wp-coding-standards\/wpcs": "^0.14.1",
        "squizlabs\/php_codesniffer": "^3.0.2",
        "mockery\/mockery": "*",
        "10up\/wp_mock": "*",
        "phpcompatibility\/php-compatibility": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "FSVendor\\Octolize\\Brand\\": "src\/Brand"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/unit"
        ]
    },
    "extra": {
        "text-domain": "wp-octolize-brand-assets",
        "translations-folder": "lang",
        "po-files": {
            "pl_PL": "pl_PL.po"
        }
    },
    "repositories": {
        "octolize": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        },
        "wpdesk": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit-unit": "phpunit --configuration phpunit-unit.xml --coverage-text --colors=never",
        "phpunit-unit-fast": "phpunit --configuration phpunit-unit.xml --no-coverage",
        "phpunit-integration": "phpunit --configuration phpunit-integration.xml --coverage-text --colors=never",
        "phpunit-integration-fast": "phpunit --configuration phpunit-integration.xml --no-coverage"
    }
}